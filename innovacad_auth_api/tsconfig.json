{
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "bundler",
		"types": [
			"bun-types"
		],
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"paths": {
			"@/*": [
				"./*"
			],
			"@/modules/*": [
				"./src/modules/index.ts"
			],
			"@/utils/*": [
				"./src/utils/*"
			]
		}
	}
}